{
  "extends": ["tslint:recommended", "tslint-plugin-prettier", "tslint-config-prettier"],
  "rules": {
    "prettier": true,
    "object-literal-sort-keys": false,
    "no-console": false,
    "interface-name": false,
    // https://palantir.github.io/tslint/rules/variable-name/
    "variable-name": {
        "options": [
            "ban-keywords",
            "check-format",
            "allow-pascal-case",
            "allow-leading-underscore"
        ]
    }
  }
}