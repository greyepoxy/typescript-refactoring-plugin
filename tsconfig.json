{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "lib",
        "strict": true,
        "lib": [
            "es6"
        ],
        "noUnusedLocals": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitThis": true
    },
    "include": [
      "src",
      "test"
    ],
}